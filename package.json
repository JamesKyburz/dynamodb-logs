{
  "name": "dynamodb-logs",
  "version": "1.0.0",
  "description": "dynamodb-logs",
  "main": "index.js",
  "keywords": [
    "lambda",
    "event-sourcing",
    "dynamodb",
    "append only",
    "serverless",
    "aws",
    "lambda"
  ],
  "author": "James Kyburz <james.kyburz@gmail.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "aws-sdk": "2.828.0",
    "commitizen": "4.2.3",
    "cz-conventional-changelog": "3.3.0",
    "husky": "4.3.8",
    "lint-staged": "10.5.3",
    "npm-check-updates": "10.2.5",
    "prettier": "2.2.1",
    "prettier-standard": "16.4.1",
    "serverless": "2.19.0",
    "serverless-dynamodb-local": "0.2.39",
    "serverless-iam-roles-per-function": "3.0.1",
    "serverless-offline": "6.8.0",
    "serverless-offline-aws-eventbridge": "git+https://github.com/JamesKyburz/serverless-offline-eventBridge.git#fix/lambda-invoke",
    "serverless-offline-dynamodb-streams": "4.2.0",
    "serverless-python-requirements": "5.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.md": "prettier --write",
    "*.yml": "prettier --write",
    "**/*.md": "prettier --write",
    "**/*.js": "prettier-standard",
    "**/*.yml": "prettier --write",
    "**/*.py": "./formatters/python.sh",
    "**/*.sh": "./formatters/sh.sh",
    "*.sh": "./formatters/sh.sh"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JamesKyburz/dynamodb-logs.git"
  },
  "bugs": {
    "url": "https://github.com/JamesKyburz/dynamodb-logs/issues"
  },
  "homepage": "https://github.com/JamesKyburz/dynamodb-logs#readme"
}
